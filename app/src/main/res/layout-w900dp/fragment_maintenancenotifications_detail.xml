        <com.sap.cloud.mobile.fiori.misc.KeyValueCell
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:keyText="NamePersonCreatedObject"
            app:valueText="@{maintenanceNotification.getOptionalValue(MaintenanceNotification.namePersonCreatedObject).toString()}" />

        <com.sap.cloud.mobile.fiori.misc.KeyValueCell
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:keyText="NamePersonReportingNotification"
