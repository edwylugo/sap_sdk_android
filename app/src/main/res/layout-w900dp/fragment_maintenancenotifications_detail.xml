<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        tools:context=".mdui.maintenancenotifications.MaintenanceNotificationsActivity">
    <data>
        <import type = "com.sap.cloud.android.odata.ipmcontainer.MaintenanceNotification" />
        <variable name="maintenanceNotification" type="MaintenanceNotification"/>
        <variable name="handler" type="com.company.mysapcpsdkproject.mdui.maintenancenotifications.MaintenanceNotificationsDetailFragment" />
    </data>

    <LinearLayout
