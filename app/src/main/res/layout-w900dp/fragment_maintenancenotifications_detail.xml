            android:layout_height="wrap_content"
            app:keyText="DateOfNotification"
            app:valueText="@{maintenanceNotification.getOptionalValue(MaintenanceNotification.dateOfNotification).toString()}" />

        <com.sap.cloud.mobile.fiori.misc.KeyValueCell
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:keyText="EndMalfunctionDate"
            app:valueText="@{maintenanceNotification.getOptionalValue(MaintenanceNotification.endMalfunctionDate).toString()}" />

        <com.sap.cloud.mobile.fiori.misc.KeyValueCell
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:keyText="EndMalfunctionTime"
            app:valueText="@{maintenanceNotification.getOptionalValue(MaintenanceNotification.endMalfunctionTime).toString()}" />

        <com.sap.cloud.mobile.fiori.misc.KeyValueCell
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:keyText="EquipmentNumber"
