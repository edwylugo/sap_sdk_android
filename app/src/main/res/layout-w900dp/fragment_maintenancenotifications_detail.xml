        <com.sap.cloud.mobile.fiori.misc.KeyValueCell
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:keyText="FunctionalLocation"
            app:valueText="@{maintenanceNotification.getOptionalValue(MaintenanceNotification.functionalLocation).toString()}" />

        <com.sap.cloud.mobile.fiori.misc.KeyValueCell
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:keyText="LocationAccountTechnical"
            app:valueText="@{maintenanceNotification.getOptionalValue(MaintenanceNotification.locationAccountTechnical).toString()}" />

        <com.sap.cloud.mobile.fiori.misc.KeyValueCell
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:keyText="MaintenancePlanningPlant"
