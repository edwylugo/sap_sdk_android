        <com.sap.cloud.mobile.fiori.misc.KeyValueCell
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:keyText="NotificationType"
            app:valueText="@{maintenanceNotification.getOptionalValue(MaintenanceNotification.notificationType).toString()}" />

        <com.sap.cloud.mobile.fiori.misc.KeyValueCell
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:keyText="ObjectID"
            app:valueText="@{maintenanceNotification.getOptionalValue(MaintenanceNotification.objectID).toString()}" />

        <com.sap.cloud.mobile.fiori.misc.KeyValueCell
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:keyText="OrderNumber"
            app:valueText="@{maintenanceNotification.getOptionalValue(MaintenanceNotification.orderNumber).toString()}" />

