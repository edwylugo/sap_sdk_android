        <com.sap.cloud.mobile.fiori.misc.KeyValueCell
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:keyText="NotificationNo"
            app:valueText="@{maintenanceNotification.getOptionalValue(MaintenanceNotification.notificationNo).toString()}" />

        <com.sap.cloud.mobile.fiori.misc.KeyValueCell
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:keyText="Assembly"
            app:valueText="@{maintenanceNotification.getOptionalValue(MaintenanceNotification.assembly).toString()}" />

        <com.sap.cloud.mobile.fiori.misc.KeyValueCell
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:keyText="BomItemNumber"
            app:valueText="@{maintenanceNotification.getOptionalValue(MaintenanceNotification.bomItemNumber).toString()}" />

        <com.sap.cloud.mobile.fiori.misc.KeyValueCell
            android:layout_width="match_parent"
