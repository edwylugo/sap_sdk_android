        <com.sap.cloud.mobile.fiori.misc.KeyValueCell
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:keyText="Priority"
            app:valueText="@{maintenanceNotification.getOptionalValue(MaintenanceNotification.priority).toString()}" />

        <com.sap.cloud.mobile.fiori.misc.KeyValueCell
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:keyText="PriorityType"
            app:valueText="@{maintenanceNotification.getOptionalValue(MaintenanceNotification.priorityType).toString()}" />

        <com.sap.cloud.mobile.fiori.misc.KeyValueCell
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:keyText="ShortText"
            app:valueText="@{maintenanceNotification.getOptionalValue(MaintenanceNotification.shortText).toString()}" />

        <com.sap.cloud.mobile.fiori.misc.KeyValueCell
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:keyText="StartMalfunctionDate"
            app:valueText="@{maintenanceNotification.getOptionalValue(MaintenanceNotification.startMalfunctionDate).toString()}" />

        <com.sap.cloud.mobile.fiori.misc.KeyValueCell
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:keyText="StartMalfunctionTime"
