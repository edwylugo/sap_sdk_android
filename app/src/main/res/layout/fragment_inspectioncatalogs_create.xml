<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">
    <data>
        <import type="com.company.mysapcpsdkproject.databinding.simplepropertyformcell.Converter"/>
        <import type="android.view.View"/>
        <variable name="inspectionCatalog" type="com.sap.cloud.android.odata.ipmcontainer.InspectionCatalog"/>
    </data>
    <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/white"
            android:clickable="true">
        <ScrollView
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_height="wrap_content"
                android:layout_width="match_parent">

            <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/create_update_inspectioncatalog"
                    android:orientation="vertical">

                <com.sap.cloud.mobile.fiori.formcell.SimplePropertyFormCell
                        xmlns:android="http://schemas.android.com/apk/res/android"
                        xmlns:app="http://schemas.android.com/apk/res-auto"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/codegroupid"
                        android:tag="CodeGroup"
                        android:maxLength="@{8}"
                        app:key="CodeGroup"
                        app:value="@={inspectionCatalog.codeGroup}">
                </com.sap.cloud.mobile.fiori.formcell.SimplePropertyFormCell>
                <com.sap.cloud.mobile.fiori.formcell.SimplePropertyFormCell
                        xmlns:android="http://schemas.android.com/apk/res/android"
                        xmlns:app="http://schemas.android.com/apk/res-auto"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/datarecordisusedid"
                        android:tag="DataRecordIsUsed"
                        android:maxLength="@{1}"
                        app:key="DataRecordIsUsed"
                        app:value="@={inspectionCatalog.dataRecordIsUsed}">
                </com.sap.cloud.mobile.fiori.formcell.SimplePropertyFormCell>
                <com.sap.cloud.mobile.fiori.formcell.SimplePropertyFormCell
                        xmlns:android="http://schemas.android.com/apk/res/android"
                        xmlns:app="http://schemas.android.com/apk/res-auto"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/datechangedrecordid"
                        android:tag="DateChangedRecord"
                        app:key="DateChangedRecord"
                        app:value="@={Converter.toString(datechangedrecordid, inspectionCatalog.dateChangedRecord, inspectionCatalog.dateChangedRecord)}">
                </com.sap.cloud.mobile.fiori.formcell.SimplePropertyFormCell>
                <com.sap.cloud.mobile.fiori.formcell.SimplePropertyFormCell
                        xmlns:android="http://schemas.android.com/apk/res/android"
                        xmlns:app="http://schemas.android.com/apk/res-auto"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/daterecordcreatedid"
                        android:tag="DateRecordCreated"
                        app:key="DateRecordCreated"
                        app:value="@={Converter.toString(daterecordcreatedid, inspectionCatalog.dateRecordCreated, inspectionCatalog.dateRecordCreated)}">
                </com.sap.cloud.mobile.fiori.formcell.SimplePropertyFormCell>
                <com.sap.cloud.mobile.fiori.formcell.SimplePropertyFormCell
                        xmlns:android="http://schemas.android.com/apk/res/android"
                        xmlns:app="http://schemas.android.com/apk/res-auto"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/nameuserchangedrecordid"
                        android:tag="NameUserChangedRecord"
                        android:maxLength="@{12}"
                        app:key="NameUserChangedRecord"
                        app:value="@={inspectionCatalog.nameUserChangedRecord}">
                </com.sap.cloud.mobile.fiori.formcell.SimplePropertyFormCell>
                <com.sap.cloud.mobile.fiori.formcell.SimplePropertyFormCell
                        xmlns:android="http://schemas.android.com/apk/res/android"
                        xmlns:app="http://schemas.android.com/apk/res-auto"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/nameusercreatedrecordid"
                        android:tag="NameUserCreatedRecord"
                        android:maxLength="@{12}"
                        app:key="NameUserCreatedRecord"
                        app:value="@={inspectionCatalog.nameUserCreatedRecord}">
                </com.sap.cloud.mobile.fiori.formcell.SimplePropertyFormCell>
                <com.sap.cloud.mobile.fiori.formcell.SimplePropertyFormCell
                        xmlns:android="http://schemas.android.com/apk/res/android"
                        xmlns:app="http://schemas.android.com/apk/res-auto"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/statusofcodegroupid"
                        android:tag="StatusOfCodeGroup"
                        android:maxLength="@{1}"
                        app:key="StatusOfCodeGroup"
                        app:value="@={inspectionCatalog.statusOfCodeGroup}">
                </com.sap.cloud.mobile.fiori.formcell.SimplePropertyFormCell>
                <com.sap.cloud.mobile.fiori.formcell.SimplePropertyFormCell
                        xmlns:android="http://schemas.android.com/apk/res/android"
                        xmlns:app="http://schemas.android.com/apk/res-auto"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/statusofmasterrecordid"
                        android:tag="StatusOfMasterRecord"
                        android:maxLength="@{1}"
                        app:key="StatusOfMasterRecord"
                        app:value="@={inspectionCatalog.statusOfMasterRecord}">
                </com.sap.cloud.mobile.fiori.formcell.SimplePropertyFormCell>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>
</layout>